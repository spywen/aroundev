{
  "name": "Aroundev",
  "version": "0.0.1",
  "homepage": "https://github.com/spywen/aroundev_project",
  "authors": [
    "spywen <spywen@hotmail.fr>"
  ],
  "license": "MIT",
  "dependencies": {
    "lodash": "~2.4.1",
    "angular": "~1.3.0",
    "angular-bootstrap": "~0.11.2",
    "angular-ui-router": "~0.2.11",
    "restangular": "~1.4.0",
    "bootstrap": "~3.2.0",
    "angular-cookies": "~1.3.0",
    "angular-translate": "~2.4.1",
    "angular-translate-loader-static-files": "~2.4.1",
    "angular-translate-storage-cookie": "~2.4.1",
    "angular-translate-loader-url": "~2.4.1",
    "font-awesome": "~4.2.0",
    "angular-animate": "~1.3.0",
    "angular-toastr": "0.5.1",
    "angular-mocks": "~1.3.1"
  },
  "overrides": {
    "angular": {
      "main": {
        "dev": "angular.js",
        "prod": [
          "angular.min.js",
          "angular.min.js.map"
        ]
      }
    },
    "angular-toastr": {
      "main": {
        "dev": [
          "dist/angular-toastr.js",
          "dist/angular-toastr.css"
